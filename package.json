{
  "name": "@seam-dev/federation-template",
  "version": "1.0.0",
  "description": "A basic webpack module federation template",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "build:watch": "NODE_ENV=production webpack --watch",
    "start": "webpack --watch",
    "dev": "webpack-dev-server",
    "serve": "serve -p 3002",
    "serve:watch": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "clean": "rm -rf dist"
  },
